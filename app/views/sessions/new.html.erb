<div class="login">
  <div class="container">
    <div class="form">

      <h1>Welcome Back</h1>
      <h3> Please log in to continue<h3>
      <%=form_for(:session, url: login_path) do |f| %>
      <%= f.email_field :email, :placeholder => "Email"%>
      <%= f.password_field :password, :placeholder => "Password"%>
      <%= f.submit :"Log In"%>
      <% end %>

    </div>
  </div>
</div>
